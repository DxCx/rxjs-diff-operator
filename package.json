{
  "name": "rxjs-diff-operator",
  "version": "0.1.1",
  "description": "toDiff/fromDiff operators for RxJs 5 - useful for sending observables over network.",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "jam": {
    "main": "./dist/main.browser.js"
  },
  "browser": {
    "dist/main.js": "./dist/main.browser.js"
  },
  "scripts": {
    "build": "webpack || echo not ok",
    "test": "jest",
    "posttest": "npm run lint",
    "test:watch": "npm test -- --watch",
    "lint": "tslint src/**/*.ts",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prerelease": "npm run build && npm test",
    "release": "standard-version && npm pack",
    "prepre-release": "npm run build && npm test",
    "pre-release": "standard-version --prerelease && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DxCx/rxjs-diff-operator.git"
  },
  "keywords": [
    "rxjs",
    "diff",
    "operators"
  ],
  "author": "Hagai Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DxCx/rxjs-diff-operator/issues"
  },
  "homepage": "https://github.com/DxCx/rxjs-diff-operator#readme",
  "devDependencies": {
    "@types/jest": "^19.2.0",
    "@types/node": "^7.0.5",
    "browserify": "^14.0.0",
    "delete-empty": "^1.0.0",
    "dts-bundle": "^0.7.2",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "rxjs": "^5.2.0",
    "standard-version": "^4.0.0",
    "ts-jest": "^19.0.2",
    "ts-loader": "^2.0.0",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "2.1.6",
    "webpack": "2.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "peerDependencies": {
    "rxjs": "^5.2.0"
  },
  "dependencies": {
    "observable-diff-operator": "^0.1.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/index.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
