{
  "name": "rxjs-diff-operator",
  "version": "0.1.0",
  "description": "toDiff/fromDiff operators for RxJs 5 - useful for sending observables over network.",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "jam": {
    "main": "./dist/main.browser.js"
  },
  "browser": {
    "dist/main.js": "./dist/main.browser.js"
  },
  "scripts": {
    "prebuild": "npm install",
    "build": "webpack || echo not ok",
    "test": "jest",
    "posttest": "npm run lint",
    "test:watch": "npm test -- --watch",
    "lint": "tslint src/**/*.ts",
    "coverage": "npm test -- --coverage --no-cache",
    "coverage:watch": "npm run coverage -- --watch",
    "prerelease": "npm run build && npm test",
    "release": "standard-version && npm pack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DxCx/rxjs-diff-operator.git"
  },
  "keywords": [
    "rxjs",
    "diff",
    "operators"
  ],
  "author": "Hagai Cohen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DxCx/rxjs-diff-operator/issues"
  },
  "homepage": "https://github.com/DxCx/rxjs-diff-operator#readme",
  "devDependencies": {
    "@types/clone": "^0.1.30",
    "@types/deep-diff": "0.0.30",
    "@types/deep-freeze": "0.0.29",
    "@types/jest": "^18.1.1",
    "@types/node": "^7.0.5",
    "awesome-typescript-loader": "^3.0.3",
    "browserify": "^14.0.0",
    "delete-empty": "^0.1.3",
    "dts-bundle": "^0.7.2",
    "jest": "^18.1.0",
    "rxjs": "^5.0.0-beta.12",
    "standard-version": "^4.0.0",
    "ts-jest": "^18.0.3",
    "tslint": "^4.4.2",
    "tslint-loader": "^3.3.0",
    "typescript": "2.1.6",
    "webpack": "2.2.1",
    "webpack-node-externals": "^1.4.3"
  },
  "peerDependencies": {
    "rxjs": ">=5.0.0-beta"
  },
  "dependencies": {
    "clone": "^2.1.0",
    "deep-diff": "^0.3.4",
    "deep-freeze": "0.0.1"
  },
  "jest": {
    "scriptPreprocessor": "node_modules/ts-jest/preprocessor.js",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/index.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
